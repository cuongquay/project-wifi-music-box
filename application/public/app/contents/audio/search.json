{
	"name": "default",
	"rows": [
		{
			"span": 12,
			"type": "container",
			"panels": [
				{
					"type": "navigator",
					"title": "{@USER}'s",
					"panels": [
						{
							"type": "search",
							"title": "Searching...",
							"class": "col-sm-12 col-md-12 col-lg-12",
							"partition": 5,
							"limit": 100
						}, {
							"type": "stream",
							"title": "Now Playing",
							"class": "col-sm-12 col-md-12 col-lg-12",
							"partition": 5,
							"limit": 40
						}, {
							"type": "running",
							"title": "Running",
							"class": "col-sm-12 col-md-12 col-lg-12"
						}
					]
				}
			]
		}
	]
}